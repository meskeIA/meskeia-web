{
  "name": "meskeia-web-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npx kill-port 3050 || exit 0",
    "dev": "next dev -p 3050",
    "prebuild": "npx playwright test tests/formatters.spec.ts && node scripts/generate-llm-index.js",
    "build": "next build",
    "start": "next start -p 3050",
    "lint": "eslint",
    "test": "npx playwright test",
    "test:unit": "npx playwright test tests/formatters.spec.ts",
    "test:a11y": "npx playwright test tests/accessibility.spec.ts",
    "generate:llm-index": "node scripts/generate-llm-index.js",
    "analyze": "set ANALYZE=true && npm run build"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "algebrite": "^1.4.0",
    "chart.js": "^4.5.1",
    "flag-icons": "^7.5.0",
    "fuse.js": "^7.1.0",
    "hls.js": "^1.6.15",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.12.1",
    "js-yaml": "^4.1.1",
    "jspdf": "^3.0.4",
    "jstat": "^1.9.6",
    "jszip": "^3.10.1",
    "mathjs": "^15.1.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "qr-code-styling": "^1.9.2",
    "react": "19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.0",
    "sql.js": "^1.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@axe-core/playwright": "^4.11.0",
    "@next/bundle-analyzer": "^16.0.6",
    "@playwright/test": "^1.56.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "typescript": "^5"
  }
